name=kdewebkit
version=5.91.0
release=1
source=(https://download.kde.org/stable/frameworks/${version/.0/}/portingAids/$name-$version.tar.xz)

build() {
	cd $name-$version

	

	cmake \
	-DCMAKE_INSTALL_PREFIX=/usr \
	-DSYSCONF_INSTALL_DIR=/etc \
	-DLIB_INSTALL_DIR=/usr/lib \
	-DBUILD_TESTING=OFF \
	-DKDE_INSTALL_USE_QT_SYS_PATHS=ON \
	-DCMAKE_BUILD_TYPE=Release \
	-Wno-dev

	make
	make DESTDIR=$PKG install

	rm -rf $PKG/usr/share/locale
	rm -rf $PKG/usr/share/man/{ca,de,it,nl,pt_BR,sv,uk}
	rm -rf $PKG/usr/share/doc
}
