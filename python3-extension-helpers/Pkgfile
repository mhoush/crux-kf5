release=2
source=(https://files.pythonhosted.org/packages/15/44/fc7754aa170b1f3b4318b009d00f1045a7f4616e2b77bcc3b3d4f5601be5/${name/python3-/}-$version.tar.gz)

build() {
	cd ${name/python3-/}-$version
	python3 -m pip install . --prefix=$PKG	
}
